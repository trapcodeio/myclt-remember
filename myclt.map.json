{
  "namespace": "remember",
  "file": "./index.ts",
  "commands": {
    "set": {
      "desc": "Set a value using a key.",
      "args": {
        "key": "Key to set.",
        "value": "Value to set."
      }
    },
    "get": {
      "desc": "Get a value using a key.",
      "args": {
        "key": "Key to get."
      }
    },
    "has": {
      "desc": "Check if a key exists.",
      "args": {
        "key": "Key to check."
      }
    },
    "unset": {
      "desc": "Unset a key.",
      "args": {
        "key": "Key to unset."
      }
    },
    "json": {
      "desc": "Show all data as json or write json to a file.",
        "args": {
            "file": "optional: File to write json to."
        }
    },
    "backup/jsonbank": {
      "desc": "Backup your data to your Json bank account.",
      "args": {
        "project": "Your json bank project name."
      }
    },
    "restore/jsonbank": {
      "desc": "Restore your data from your Json bank account.",
      "args": {
        "project": "Your json bank project name."
      }
    }
  }
}